{
  "expo": {
    "name": "Morrow",
    "slug": "morrow",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.morrow.app"
    },
    "android": {
      "package": "com.morrow.app"
    },
    "web": {
      "bundler": "webpack",
      "output": "web-build"
    },
    "extra": {
      "apiUrl": "http://localhost:8080/api/v1"
    }
  }
}
